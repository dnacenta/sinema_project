
<%= link_to "#{@user.name}", {controller: 'users', action: 'show', id: @user.id} %>

<h2><%=@group.name%></h2>
<ul>
  <% @group.users.each do |user| %>
    <li><%= user.name %></li>
  <%end%>
</ul>
<%= form_for add_user_url do |f| %>
  <%= f.text_field :name, placeholder: "Username" %>
  <%= f.submit 'Add User' %>
<% end %>

<ul id="img"></ul>
