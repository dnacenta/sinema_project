<h2><%=@group.name%></h2>

<button class="btn btn-success" type="button">
  <%= link_to add_user_path(@user, @group), method: :get do %>
    Add User
  <% end %>
</button>

<button class="btn btn-danger" type="button">
  <%= link_to exit_group_path(@user, @group), method: :get do %>
    Exit Group
  <% end %>
</button>

<button class="btn btn-default" type="button">
  <%= link_to user_group_choices_path(@user, @group), method: :get do %>
    Your Choices
  <% end %>
</button>

